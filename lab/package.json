{
  "name": "clawlist-lab",
  "version": "1.0.0",
  "description": "TypeScript test framework for autonomous AI agent marketplace negotiation",
  "keywords": ["ai", "agent", "negotiation", "marketplace", "matrix", "testing", "security"],
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "validate": "npm run build && node dist/cli-validate-scenarios.js"
  },
  "dependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.18",
    "vitest": "^4.0.18"
  }
}
